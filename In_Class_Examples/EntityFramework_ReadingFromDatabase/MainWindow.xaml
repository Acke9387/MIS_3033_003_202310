<Window x:Class="EntityFramework_ReadingFromDatabase.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EntityFramework_ReadingFromDatabase"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="lstAway" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="507,0,0,0"/>
        <Button x:Name="btnCollectData" Content="Collect Data" HorizontalAlignment="Left" Margin="109,188,0,0" VerticalAlignment="Top" Height="56" Width="109" Click="btnCollectData_Click"/>
        <ListBox x:Name="lstHome" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="214,0,293,0"/>

    </Grid>
</Window>
